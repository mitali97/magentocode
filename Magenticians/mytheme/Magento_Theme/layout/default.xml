<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<body>
	<referenceContainer name="footer-container" remove="true">
	</referenceContainer>
	<referenceBlock name="top.search" remove="true"></referenceBlock>
	<referenceBlock name="header.links" remove="true"></referenceBlock>
	<referenceBlock name="header.panel.wrapper" remove="true"></referenceBlock>
	<referenceBlock name="copyright" remove="true"></referenceBlock>
	<referenceBlock name="catalog.compare.sidebar" remove="true"></referenceBlock>
	<!--  <move element="catalog.topnav" destination="header-wrapper" after="logo"/> -->
	
	<!-- <move element="minicart" destination="header-wrapper" /> -->
    <referenceBlock name="catalog.compare.sidebar" remove="true"/>
	<referenceBlock name="breadcrumbs" remove="true" />
    <referenceBlock name="product_list_toolbar" display="false" />
    <referenceBlock name="page.main.title" display="false"/>

